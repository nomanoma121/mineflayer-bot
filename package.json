{
  "name": "mineflayer-bot",
  "version": "2.0.0",
  "description": "A versatile Minecraft bot built with TypeScript and design patterns",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "cli": "ts-node src/cli/BotCLI.ts",
    "bot:start": "npm run cli start",
    "bot:config": "npm run cli config show",
    "bot:test": "npm run cli dev test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nomanoma121/mineflayer-bot.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nomanoma121/mineflayer-bot/issues"
  },
  "homepage": "https://github.com/nomanoma121/mineflayer-bot#readme",
  "dependencies": {
    "chokidar": "^4.0.3",
    "commander": "^14.0.0",
    "dotenv": "^16.6.1",
    "minecraft-data": "^3.51.0",
    "mineflayer": "^4.29.0",
    "mineflayer-pathfinder": "^2.4.5",
    "prismarine-entity": "^2.5.0",
    "tsyringe": "^4.10.0",
    "vec3": "^0.1.10",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/commander": "^2.12.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
